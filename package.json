{
    "name": "align-by-regex",
    "displayName": "Align by RegEx",
    "description": "Aligns selected lines of text by a regular expression.",
    "version": "1.0.5",
    "icon": "images/icon.png",
    "publisher": "janjoerke",
    "license": "LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/janjoerke/vscode-align-by-regex.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:align.by.regex"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "align.by.regex",
                "title": "Align by RegEx"
            }
        ],
        "keybindings": [
            {
                "command": "align.by.regex",
                "key": "alt+shift+a",
                "when": "editorHasSelection"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Align by RegEx",
            "properties": {
                "align.by.regex.templates": {
                    "type": "object",
                    "description": "Stored RegEx templates."
                },
                "align.by.regex.ignoreFocusOut": {
                    "type": "boolean",
                    "default": false,
                    "description": "When enabled input boxes created by this extension's commands will remain open when focus is lost.",
                    "markdownDescription": "When enabled input boxes created by this extension's commands will remain open when focus is lost."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
